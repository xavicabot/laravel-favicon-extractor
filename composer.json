{
    "name": "xavicabot/laravel-favicon-extractor",
    "description": "A favicon extractor for Laravel",
    "keywords": [
        "xavicabot",
        "laravel-favicon-extractor",
        "favicon"
    ],
    "homepage": "https://github.com/xavicabot/laravel-favicon-extractor",
    "license": "MIT",
    "authors": [
        {
            "name": "Xavi Cabot",
            "email": "xavi@unancor.com",
            "homepage": "https://www.unancor.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^6|^7|^8|^9|^10",
        "graham-campbell/guzzle-factory": "^3.0|^4.0|^5.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9 || ^10",
        "orchestra/testbench": "~3.5.0|~3.6.0",
        "mockery/mockery": "^1.1",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "XaviCabot\\LaravelFaviconExtractor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XaviCabot\\LaravelFaviconExtractor\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "XaviCabot\\LaravelFaviconExtractor\\FaviconExtractorServiceProvider"
            ],
            "aliases": {
                "FaviconExtractor": "XaviCabot\\LaravelFaviconExtractor\\Facades\\FaviconExtractor"
            }
        }
    }
}
